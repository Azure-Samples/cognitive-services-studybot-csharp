  <appSettings>
    <!-- Add your bot's Microsoft App Id and Microsoft App Password from settings in Azure-->
    <add key="MicrosoftAppId" value="ENTER MICROSOFT APP ID HERE" />
    <add key="MicrosoftAppPassword" value="ENTER MICROSOFT APP PASSWORD HERE" />
    <add key="AzureWebJobsStorage" value="ENTER AZUREWEBJOBSSTORAGE HERE" />
    
    <!-- Add LUIS keys from bot settings in Azure -->
    <add key="LuisAppId" value="ENTER LUIS APP ID HERE" />
    <add key="LuisAPIKey" value="ENTER LUIS API / AUTHORING KEY HERE" />
    <add key="LuisAPIHostName" value="ENTER LUIS HOST NAME HERE" />

    <add key="BingSpellCheckSubscriptionKey" value="ENTER BINGSPELLCHECK SUBSCRIPTION KEY HERE" />

    <!-- Add QnA Maker knowledge base keys from qnamaker.ai-->
	  <add key="BiologyQnAEndpointHostName" value="ENTER KNOWLEDGE BASE 1 HOSTNAME HERE" />
	  <add key="BiologyKBId" value="ENTER KNOWLEDGE BASE 1 ID HERE" />
	  <add key="BiologyQnAAuthKey" value="ENTER KNOWLEDGE BASE 1 AUTH KEY HERE" />

    <add key="SociologyQnAEndpointHostName" value="ENTER KNOWLEDGE BASE 2 HOSTNAME HERE" />
    <add key="SociologyKBId" value="ENTER KNOWLEDGE BASE 2 ID HERE" />
    <add key="SociologyQnAAuthKey" value="ENTER KNOWLEDGE BASE 2 AUTH KEY HERE" />

    <add key="GeologyQnAEndpointHostName" value="ENTER KNOWLEDGE BASE 3 HOSTNAME HERE" />
    <add key="GeologyKBId" value="ENTER KNOWLEDGE BASE 3 ID HERE" />
    <add key="GeologyQnAAuthKey" value="ENTER KNOWLEDGE BASE 3 AUTH KEY HERE" />
  
  </appSettings>
  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.6" />
      </system.Web>
